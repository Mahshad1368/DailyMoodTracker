	<dict>
		<key>CFBundleAlternateIcons</key>
		<dict>
			<key>AppIcon-Happy</key>
			<dict>
				<key>CFBundleIconFiles</key>
				<array>
					<string>AppIcon-Happy</string>
				</array>
				<key>UIPrerenderedIcon</key>
				<false/>
			</dict>
			<key>AppIcon-Neutral</key>
			<dict>
